{
  "rabbit_version": "3.13",
  "users": [
    {
      "name": "logiflow",
      "password": "logiflow123",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "logiflow",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "logiflow.events",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "logiflow.dlx",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "pedido.creado",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "logiflow.dlx",
        "x-dead-letter-routing-key": "pedido.creado.dlq"
      }
    },
    {
      "name": "pedido.estado.actualizado",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 10000,
        "x-dead-letter-exchange": "logiflow.dlx",
        "x-dead-letter-routing-key": "pedido.estado.actualizado.dlq"
      }
    },
    {
      "name": "repartidor.ubicacion.actualizada",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 50000,
        "x-dead-letter-exchange": "logiflow.dlx",
        "x-dead-letter-routing-key": "repartidor.ubicacion.dlq"
      }
    },
    {
      "name": "pedido.creado.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "pedido.estado.actualizado.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "repartidor.ubicacion.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "logiflow.events",
      "vhost": "/",
      "destination": "pedido.creado",
      "destination_type": "queue",
      "routing_key": "pedido.creado",
      "arguments": {}
    },
    {
      "source": "logiflow.events",
      "vhost": "/",
      "destination": "pedido.estado.actualizado",
      "destination_type": "queue",
      "routing_key": "pedido.estado.actualizado",
      "arguments": {}
    },
    {
      "source": "logiflow.events",
      "vhost": "/",
      "destination": "repartidor.ubicacion.actualizada",
      "destination_type": "queue",
      "routing_key": "repartidor.ubicacion.actualizada",
      "arguments": {}
    },
    {
      "source": "logiflow.dlx",
      "vhost": "/",
      "destination": "pedido.creado.dlq",
      "destination_type": "queue",
      "routing_key": "pedido.creado.dlq",
      "arguments": {}
    },
    {
      "source": "logiflow.dlx",
      "vhost": "/",
      "destination": "pedido.estado.actualizado.dlq",
      "destination_type": "queue",
      "routing_key": "pedido.estado.actualizado.dlq",
      "arguments": {}
    },
    {
      "source": "logiflow.dlx",
      "vhost": "/",
      "destination": "repartidor.ubicacion.dlq",
      "destination_type": "queue",
      "routing_key": "repartidor.ubicacion.dlq",
      "arguments": {}
    }
  ]
}
